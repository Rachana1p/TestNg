<?xml version="1.0" encoding="UTF-8"?>
<suite name="eCom Webapp Test Suite">
	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>
	<test name="Amazon Test">
		<classes>
			<class name="amazon.AmazonHomePageTest"></class>
			<class
				name="amazon.AmazonLinkVerificationCssSelectorTest"></class>
			<class
				name="amazon.AmazonLinkVerificationTest"></class>
			<class
				name="amazon.AmazonLinkVerificationXpathTest"></class>
			<class name="amazon.AmazonSearchTest"></class>
		</classes>
	</test>
	<test name="Group Test">
		<classes>
			<class name="testng.AttributeGroupTest"></class>
		</classes>
	</test>
	<test name="Other Test">
		<classes>
			<class
				name="testng.TestNgStandardAnnotationTest"></class>
			<class name="testng.AssertionTest"></class>
		</classes>
	</test>
</suite>